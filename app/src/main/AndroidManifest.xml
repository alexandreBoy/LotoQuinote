<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alexandre.boyer.lotoquinote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lotoquinote_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lotoquinote_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.DrawListActivity" />
        <activity android:name=".controller.ModifyPopUpActivity"
            android:theme="@style/AppTheme.PopUpTheme"
        />


        <activity android:name=".controller.DrawTrackingActivity" />
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>